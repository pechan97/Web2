@model IEnumerable<Web.Models.Cliente>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo Cliente", "Create")
</p>
<table class="table table-hover table-bordered">
    <tr>
        <th class="active">
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th class="active">
            @Html.DisplayNameFor(model => model.CedulaJuridica)
        </th>
        <th class="active">
            @Html.DisplayNameFor(model => model.PaginaWeb)
        </th>
        <th class="active">
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th class="active">
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th class="active">
            @Html.DisplayNameFor(model => model.Sector)
        </th>
        <th class="active"></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CedulaJuridica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaginaWeb)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sector)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
